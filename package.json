{
  "name": "s-ng-dev-utils-platform",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postbuild": "cpx \"{tslint*.json,.eslintrc.js}\" dist/s-ng-dev-utils/",
    "test": "ng test",
    "lint": "ng lint && eslint . --ext .js,.jsx,.ts,.tsx",
    "e2e": "ng e2e",
    "dtslint": "dtslint --expectOnly --localTs node_modules/typescript/lib projects/s-ng-dev-utils/src/typing-tests",
    "docs": "cd docs && typedoc ../projects/s-ng-dev-utils/src/lib",
    "prettier-all": "prettier --write \"**/*.{html,js,json,md,scss,ts,yml}\"",
    "release": "yarn docs && git add docs/* && standard-version --commit-all && yarn build --prod s-ng-dev-utils"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "tslint-config-prettier": "^1.18.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/prettier": "^1.18.2",
    "codelyzer": "^6.0.0-next.1",
    "coveralls": "^3.0.6",
    "cpx": "^1.5.0",
    "dtslint": "^2.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "prettier": "^1.18.2",
    "protractor": "~7.0.0",
    "standard-version": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typedoc": "^0.16.9",
    "typescript": "~3.9.5"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "standard-version": {
    "scripts": {
      "postbump": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" standard-version-postbump",
      "postchangelog": "prettier --write CHANGELOG.md",
      "precommit": "git add projects/s-ng-dev-utils/package.json"
    }
  }
}
