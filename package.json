{
  "name": "s-ng-dev-utils-platform",
  "version": "0.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postbuild": "cpx tslint*.json dist/s-ng-dev-utils/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "cd docs && typedoc ../projects/s-ng-dev-utils/src/lib",
    "prettier-all": "prettier --write \"**/*.{html,js,json,md,scss,ts,yml}\"",
    "release": "yarn docs && git add docs/* && standard-version --commit-all && yarn build s-ng-dev-utils"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0-rc.2",
    "@angular/common": "~8.0.0-rc.2",
    "@angular/compiler": "~8.0.0-rc.2",
    "@angular/core": "~8.0.0-rc.2",
    "@angular/forms": "~8.0.0-rc.2",
    "@angular/platform-browser": "~8.0.0-rc.2",
    "@angular/platform-browser-dynamic": "~8.0.0-rc.2",
    "@angular/router": "~8.0.0-rc.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "tslint-config-prettier": "^1.18.0",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-rc.2",
    "@angular-devkit/build-ng-packagr": "~0.800.0-rc.2",
    "@angular/cli": "~8.0.0-rc.3",
    "@angular/compiler-cli": "~8.0.0-rc.2",
    "@angular/language-service": "~8.0.0-rc.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/prettier": "^1.16.2",
    "codelyzer": "~5.0.0",
    "coveralls": "^3.0.3",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "prettier": "^1.17.0",
    "protractor": "~5.4.0",
    "standard-version": "^6.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typedoc": "^0.14.2",
    "typescript": "~3.4.3"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "standard-version": {
    "scripts": {
      "postbump": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" standard-version-postbump",
      "postchangelog": "prettier --write CHANGELOG.md",
      "precommit": "git add projects/s-ng-dev-utils/package.json"
    }
  }
}
